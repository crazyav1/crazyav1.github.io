    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"### Key Java Implementation\\n\",\n",
    "    \"```java\\n\",\n",
    "    \"// TimeSeriesPlotter.java\\n\",\n",
    "    \"public void plotDemandPvPriceFirstDays(List<DataRow> rows) {\\n\",\n",
    "    \"    var window = firstDays(rows, cfg.days());\\n\",\n",
    "    \"    var points = align(window);\\n\",\n",
    "    \"\\n\",\n",
    "    \"    var chart = chart(\\\"Prices, Demand and PV - first %d days\\\".formatted(cfg.days()));\\n\",\n",
    "    \"    chart.addSeries(\\\"Demand (kW)\\\", points.time(), points.demand());\\n\",\n",
    "    \"    chart.addSeries(\\\"PV (kW)\\\", points.time(), points.pv());\\n\",\n",
    "    \"\\n\",\n",
    "    \"    chart.addSeries(\\\"Price (Euro/kWh)\\\", points.timePrice(), points.price())\\n\",\n",
    "    \"      .setYAxisGroup(1)\\n\",\n",
    "    \"      .setFillColor(Color.RED);\\n\",\n",
    "    \"\\n\",\n",
    "    \"    ChartExporter.saveSvg(chart, out, \\\"timeseries_demand_pv_price\\\");\\n\",\n",
    "    \"}\\n\",\n",
    "    \"```\"\n",
