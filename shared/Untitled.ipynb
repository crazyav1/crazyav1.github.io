    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## Task 6: Decomposition\\n\",\n",
    "    \"\\n\",\n",
    "    \"Classical additive decomposition.\\n\",\n",
    "    \"\\n\",\n",
    "    \"### Key Java Implementation (ClassicalDecomposition.java)\\n\",\n",
    "    \"\\n\",\n",
    "    \"```java\\n\",\n",
    "    \"// Trend: Moving Average\\n\",\n",
    "    \"double[] trend = MovingAverage.centered(observed, window);\\n\",\n",
    "    \"\\n\",\n",
    "    \"// Seasonality: Average detrended by hour\\n\",\n",
    "    \"double[] seasonal = new double[n];\\n\",\n",
    "    \"for (int i=0; i<n; i++) seasonal[i] = seasonalIndices[i % period];\\n\",\n",
    "    \"\\n\",\n",
    "    \"// Residual\\n\",\n",
    "    \"double[] residual = new double[n];\\n\",\n",
    "    \"for (int i=0; i<n; i++) residual[i] = observed[i] - trend[i] - seasonal[i];\\n\",\n",
    "    \"```\"\n",
    "   ]\n",
